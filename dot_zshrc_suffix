
setopt NO_HUP
setopt NO_CHECK_JOBS

export                  BOOST_TEST_BUILD_INFO=yes
export                BOOST_TEST_COLOR_OUTPUT=yes
export                   BOOST_TEST_LOG_LEVEL=message
export                BOOST_TEST_REPORT_LEVEL=short
export              BUILD_EXTRA_CATH_BINARIES=1
export                              BUILDTYPE=ninja_clang_relwithdebinfo
export                CTEST_OUTPUT_ON_FAILURE=1
export MXX_ALWAYS_THROW_ON_CONTRACT_VIOLATION=1
export                                   PATH=~/bin:~/puppet/perl/tools:$PATH:/usr/local/cuda-5.0/bin

# export                DBIC_TRACE_PROFILE=console
# export                            EDITOR=vim

# Resort to, eg, put the [...]_test.cpp before the [...].cpp
#
# Use like `find -iname '*.cpp' | mysortpipe`
#
# This appears before the other aliases so they can use it
alias       mysortpipe='sed "s/_test\./#test\./g" | env LC_ALL=C sort | tr "#" "_" '

alias             auto='libtoolize --force;aclocal;autoconf;touch NEWS AUTHORS README ChangeLog;automake --add-missing'
alias        datestamp='date +%Y%m%d'
alias   gen_cmake_list='~/puppet/perl/cpan/Butler/scripts/gen_cmake_list.pl $PWD'
alias              lgt='( git --no-pager log --color --all --decorate --oneline --graph --format=format:"%C(auto)%h%C(reset)%C(auto)%d%C(reset) %C(white)%s%C(reset)   %C(blue)%an, %ar%C(reset)" && echo ) | tac | perl -pe '\''s/(\w+)\\([^[:print:]])/\1#\2/g; s/(\w+)\/([^[:print:]])/\1\\\2/g; s/(\w+)#([^[:print:]])/\1\/\2/g;'\'' '
alias         lsbigmov='find . \( -name "*.mp4" -o -name "*.wmv" -o -name "*.mp*g" -o -name "*.avi" -o -name "*.mov" -o -name "*.flv" -o -name "*.asf" \) -a -size +30M'
alias              lsc='find . \( -iname "*.pl" -o -iname "*.pm" -o -iname "*.t" -o -name "*.c" -o -name "*.cpp" -o -name "*.cu" -o -name "*.cuh" -o -name "*.cxx" -o -name "*.h" -o -name "*.hpp" -o -name "*.tex" \) -print | grep -vP "\/\.backup\." | grep -vP "\/\.kate-backup" | sed "s/^\.\///g"                                                                                  | mysortpipe '
alias              msc='find . \( -iname "*.pl" -o -iname "*.pm" -o -iname "*.t" -o -name "*.c" -o -name "*.cpp" -o -name "*.cu" -o -name "*.cuh" -o -name "*.cxx" -o -name "*.h" -o -name "*.hpp" -o -name "*.tex" \) -print | grep -vP "\/\.backup\." | grep -vP "\/\.kate-backup" | sed "s/^\.\///g" | grep -vP "^(juce|build_|ninja_|3rdparty|autogen|other_stuff|build-deps-graph)" | mysortpipe '
alias            lscol='ls --color=always '
alias            lsmov='find . -name "*.mp4" -o -name "*.wmv" -o -name "*.mp*g" -o -name "*.avi" -o -name "*.mov" -o -name "*.flv" -o -name "*.asf"'
alias              lsp='find script lib t -iname "*.pm" -o -iname "*.pl" -o -name "*.t" | mysortpipe '
alias              lsq='find . -iname "*.qml" | mysortpipe '
alias              lsy='find . -iname "*.py" | mysortpipe '
alias             lx53='cd / ; ssh bsmlx53'
alias             lx62='cd / ; ssh bsmlx62'
alias        timestamp='date +%Y%m%d-%H%M%S'
alias             wget='wget -U Firefox --content-disposition'

function cmake_all_ninjas() {
	rm    -rf ninja_clang_debug_shared ninja_clang_release ninja_clang_relwithdebinfo ninja_gcc_debug_shared ninja_gcc_release ninja_gcc_relwithdebinfo
	mkdir -p  ninja_clang_debug_shared ninja_clang_release ninja_clang_relwithdebinfo ninja_gcc_debug_shared ninja_gcc_release ninja_gcc_relwithdebinfo
	(cd ninja_clang_debug_shared   && /usr/bin/cmake -GNinja -DCMAKE_BUILD_TYPE=DEBUG          -DBUILD_SHARED_LIBS=ON -DBUILD_EXTRA_CATH_TOOLS=ON -DBUILD_EXTRA_CATH_TESTS=ON -DBOOST_ROOT=/opt/boost_1_67_0_clang_c++14_build -DCMAKE_C_COMPILER=/usr/bin/clang -DCMAKE_CXX_COMPILER=/usr/bin/clang++ -DCMAKE_CXX_FLAGS="-stdlib=libc++" .. )
	(cd ninja_clang_release        && /usr/bin/cmake -GNinja -DCMAKE_BUILD_TYPE=RELEASE                               -DBUILD_EXTRA_CATH_TOOLS=ON -DBUILD_EXTRA_CATH_TESTS=ON -DBOOST_ROOT=/opt/boost_1_67_0_clang_c++14_build -DCMAKE_C_COMPILER=/usr/bin/clang -DCMAKE_CXX_COMPILER=/usr/bin/clang++ -DCMAKE_CXX_FLAGS="-stdlib=libc++" .. )
	(cd ninja_clang_relwithdebinfo && /usr/bin/cmake -GNinja -DCMAKE_BUILD_TYPE=RELWITHDEBINFO                        -DBUILD_EXTRA_CATH_TOOLS=ON -DBUILD_EXTRA_CATH_TESTS=ON -DBOOST_ROOT=/opt/boost_1_67_0_clang_c++14_build -DCMAKE_C_COMPILER=/usr/bin/clang -DCMAKE_CXX_COMPILER=/usr/bin/clang++ -DCMAKE_CXX_FLAGS="-stdlib=libc++" .. )
	(cd ninja_gcc_debug_shared     && /usr/bin/cmake -GNinja -DCMAKE_BUILD_TYPE=DEBUG          -DBUILD_SHARED_LIBS=ON -DBUILD_EXTRA_CATH_TOOLS=ON -DBUILD_EXTRA_CATH_TESTS=ON -DBOOST_ROOT=/opt/boost_1_67_0_gcc_c++14_build                                                                                                              .. )
	(cd ninja_gcc_release          && /usr/bin/cmake -GNinja -DCMAKE_BUILD_TYPE=RELEASE                               -DBUILD_EXTRA_CATH_TOOLS=ON -DBUILD_EXTRA_CATH_TESTS=ON -DBOOST_ROOT=/opt/boost_1_67_0_gcc_c++14_build                                                                                                              .. )
	(cd ninja_gcc_relwithdebinfo   && /usr/bin/cmake -GNinja -DCMAKE_BUILD_TYPE=RELWITHDEBINFO                        -DBUILD_EXTRA_CATH_TOOLS=ON -DBUILD_EXTRA_CATH_TESTS=ON -DBOOST_ROOT=/opt/boost_1_67_0_gcc_c++14_build                                                                                                              .. )
}
function xlomp {
	echo "\033[1;34mClang compiling $@\033[0m"
	clang++ -x c++ -Iautogen/mxx_pro_git_version -I/mxx-pro/desktop -isystem /usr/include/x86_64-linux-gnu -isystem /mxx-pro/autogen/api -isystem /home/lewis/source/cpprestsdk-clang-build/include -I/mxx-pro/arrange/lib_arrange -I/mxx-pro/annotation/lib_annotation -isystem /mxx-pro/3rdparty -I/mxx-pro/time/lib_time -I/mxx-pro/utility/lib_utility -isystem /opt/boost_1_67_0_clang_c++14_build/include -I/mxx-pro/audio/lib_audio -isystem /mxx-pro/juce -I/mxx-pro/mixer/lib_mixer -isystem /home/lewis/Qt/5.10.1/gcc_64/include -isystem /home/lewis/Qt/5.10.1/gcc_64/include/QtQuick -isystem /home/lewis/Qt/5.10.1/gcc_64/include/QtQml -isystem /home/lewis/Qt/5.10.1/gcc_64/include/QtNetwork -isystem /home/lewis/Qt/5.10.1/gcc_64/include/QtCore -isystem /home/lewis/Qt/5.10.1/gcc_64/./mkspecs/linux-g++ -isystem /home/lewis/Qt/5.10.1/gcc_64/include/QtGui -isystem /home/lewis/Qt/5.10.1/gcc_64/include/QtWidgets -isystem /home/lewis/Qt/5.10.1/gcc_64/include/QtAV -isystem /home/lewis/Qt/5.10.1/gcc_64/include -stdlib=libc++ -fcolor-diagnostics -W -Wall -Werror -Wextra -Wcast-qual -Wconversion -Wformat=2 -Wnon-virtual-dtor -Wshadow -Wsign-compare -Wsign-conversion -pedantic -ftabstop=2 -std=c++14 -o /tmp/.xomp.dummy.o -c $@
}
function xomp {
	echo "\033[1;34mClang compiling $@\033[0m"
	g++     -x c++ -Iautogen/mxx_pro_git_version -I/mxx-pro/desktop -isystem /usr/include/x86_64-linux-gnu -isystem /mxx-pro/autogen/api -isystem /home/lewis/source/cpprestsdk-clang-build/include -I/mxx-pro/arrange/lib_arrange -I/mxx-pro/annotation/lib_annotation -isystem /mxx-pro/3rdparty -I/mxx-pro/time/lib_time -I/mxx-pro/utility/lib_utility -isystem /opt/boost_1_67_0_clang_c++14_build/include -I/mxx-pro/audio/lib_audio -isystem /mxx-pro/juce -I/mxx-pro/mixer/lib_mixer -isystem /home/lewis/Qt/5.10.1/gcc_64/include -isystem /home/lewis/Qt/5.10.1/gcc_64/include/QtQuick -isystem /home/lewis/Qt/5.10.1/gcc_64/include/QtQml -isystem /home/lewis/Qt/5.10.1/gcc_64/include/QtNetwork -isystem /home/lewis/Qt/5.10.1/gcc_64/include/QtCore -isystem /home/lewis/Qt/5.10.1/gcc_64/./mkspecs/linux-g++ -isystem /home/lewis/Qt/5.10.1/gcc_64/include/QtGui -isystem /home/lewis/Qt/5.10.1/gcc_64/include/QtWidgets -isystem /home/lewis/Qt/5.10.1/gcc_64/include/QtAV -isystem /home/lewis/Qt/5.10.1/gcc_64/include                 -fdiagnostics-color -W -Wall -Werror -Wextra -Wcast-qual -Wconversion -Wformat=2 -Wnon-virtual-dtor -Wshadow -Wsign-compare -Wsign-conversion -pedantic -ftabstop=2 -std=c++14 -o /tmp/.xomp.dummy.o -c $@
}
function clomp() {
	echo "\033[1;34mClang compiling $@\033[0m"
	clang++ -DBOOST_LOG -std=c++17 -stdlib=libc++ -fcolor-diagnostics -ftemplate-depth=400 -W -Wall -Werror -Wextra -Wno-unused-local-typedef -Wsign-compare -Wcast-qual -Wconversion -Wnon-virtual-dtor -pedantic -ftemplate-backtrace-limit=0             -S        -o /tmp/.comp_clang.dummy.o -isystem /opt/include -isystem range-v3/include -isystem rapidjson/include -I /usr/local/cuda/include -I source/uni -I source/src_clustagglom -I source/src_common -I source/src_test -I source $@ |& sed 's/source\///g'
}
function comp() {
	echo "\033[1;34mGCC compiling $@\033[0m"
	g++     -DBOOST_LOG -std=c++17                                                         -W -Wall -Werror -Wextra                           -Wsign-compare -Wcast-qual -Wconversion -Wnon-virtual-dtor -pedantic -ftemplate-backtrace-limit=0 -ftabstop=2 -S -x c++ -o /tmp/.comp_gcc.dummy.o   -isystem /opt/include -isystem range-v3/include -isystem rapidjson/include -I /usr/local/cuda/include -I source/uni -I source/src_clustagglom -I source/src_common -I source/src_test -I source $@ |& head -n 800 |& ~/puppet/perl/tools/makeoutputparser.pl
}
function makeall() {
	ls -1d build_* | xargs -I VAR make -C VAR -k $@
}
function makec() {
	make $@ |& ~/puppet/perl/tools/makeoutputparser.pl
}
function nar() {
	ninja         $1 -k 0    -C $BUILDTYPE                                          &&            $BUILDTYPE/$1 $@[2,-1]
}
function nar-debug() {
	ninja         $1 -k 0    -C $BUILDTYPE                                          && gdb --args $BUILDTYPE/$1 $@[2,-1]
}
function narc() {
	ninja         $1 -k 0    -C $BUILDTYPE |& ~/puppet/perl/tools/makeoutputparser.pl &&            $BUILDTYPE/$1 $@[2,-1]
}
function narc-debug() {
	ninja         $1 -k 0    -C $BUILDTYPE |& ~/puppet/perl/tools/makeoutputparser.pl && gdb --args $BUILDTYPE/$1 $@[2,-1]
}
function ninjaall() {
	ls -1d ninja_* | xargs -I VAR ninja -C VAR -k 0 $@
}
function ninjac() {
	ninja $@ | ~/puppet/perl/tools/makeoutputparser.pl
}
function qmake_all_makes() {
	rm    -rf build_clang_debug build_clang_release build_clang_profile build_gcc_debug build_gcc_release build_gcc_profile
	mkdir -p  build_clang_debug build_clang_release build_clang_profile build_gcc_debug build_gcc_release build_gcc_profile
	(cd build_clang_debug   && ~/Qt/5.10.1/gcc_64/bin/qmake CONFIG+=debug   'QMAKE_CXXFLAGS += -isystem /opt/boost_1_66_0_clang_c++14_build/include -isystem /home/lewis/source/cpprestsdk-clang-build/include' 'LIBS=-Wl,-rpath=/opt/boost_1_66_0_clang_c++14_build/lib -L/opt/boost_1_66_0_clang_c++14_build/lib -Wl,-rpath=/home/lewis/source/cpprestsdk-clang-build/lib -L/home/lewis/source/cpprestsdk-clang-build/lib ' -spec linux-clang-libc++ .. )
	(cd build_clang_release && ~/Qt/5.10.1/gcc_64/bin/qmake CONFIG+=release 'QMAKE_CXXFLAGS += -isystem /opt/boost_1_66_0_clang_c++14_build/include -isystem /home/lewis/source/cpprestsdk-clang-build/include' 'LIBS=-Wl,-rpath=/opt/boost_1_66_0_clang_c++14_build/lib -L/opt/boost_1_66_0_clang_c++14_build/lib -Wl,-rpath=/home/lewis/source/cpprestsdk-clang-build/lib -L/home/lewis/source/cpprestsdk-clang-build/lib ' -spec linux-clang-libc++ .. )
	(cd build_clang_profile && ~/Qt/5.10.1/gcc_64/bin/qmake CONFIG+=profile 'QMAKE_CXXFLAGS += -isystem /opt/boost_1_66_0_clang_c++14_build/include -isystem /home/lewis/source/cpprestsdk-clang-build/include' 'LIBS=-Wl,-rpath=/opt/boost_1_66_0_clang_c++14_build/lib -L/opt/boost_1_66_0_clang_c++14_build/lib -Wl,-rpath=/home/lewis/source/cpprestsdk-clang-build/lib -L/home/lewis/source/cpprestsdk-clang-build/lib ' -spec linux-clang-libc++ .. )
	(cd build_gcc_debug     && ~/Qt/5.10.1/gcc_64/bin/qmake CONFIG+=debug   'QMAKE_CXXFLAGS += -isystem /opt/boost_1_66_0_gcc_c++14_build/include   -isystem /home/lewis/source/cpprestsdk-gcc-build/include'   'LIBS=-Wl,-rpath=/opt/boost_1_66_0_gcc_c++14_build/lib   -L/opt/boost_1_66_0_gcc_c++14_build/lib   -Wl,-rpath=/home/lewis/source/cpprestsdk-gcc-build/lib   -L/home/lewis/source/cpprestsdk-gcc-build/lib   '                          .. )
	(cd build_gcc_release   && ~/Qt/5.10.1/gcc_64/bin/qmake CONFIG+=release 'QMAKE_CXXFLAGS += -isystem /opt/boost_1_66_0_gcc_c++14_build/include   -isystem /home/lewis/source/cpprestsdk-gcc-build/include'   'LIBS=-Wl,-rpath=/opt/boost_1_66_0_gcc_c++14_build/lib   -L/opt/boost_1_66_0_gcc_c++14_build/lib   -Wl,-rpath=/home/lewis/source/cpprestsdk-gcc-build/lib   -L/home/lewis/source/cpprestsdk-gcc-build/lib   '                          .. )
	(cd build_gcc_profile   && ~/Qt/5.10.1/gcc_64/bin/qmake CONFIG+=profile 'QMAKE_CXXFLAGS += -isystem /opt/boost_1_66_0_gcc_c++14_build/include   -isystem /home/lewis/source/cpprestsdk-gcc-build/include'   'LIBS=-Wl,-rpath=/opt/boost_1_66_0_gcc_c++14_build/lib   -L/opt/boost_1_66_0_gcc_c++14_build/lib   -Wl,-rpath=/home/lewis/source/cpprestsdk-gcc-build/lib   -L/home/lewis/source/cpprestsdk-gcc-build/lib   '                          .. )
}

unsetopt share_history

# Commands to remember to use:
#   cloc    Count Lines Of Code
#   column  Form input into aligned columns
#   combine Like comm but more general (and/not/or/xor; files needn't be pre-sorted) (in Ubuntu package 'moreutils')
#   comm    Find common lines in sorted files
#   entr    Rerun a command whenever any of a bunch of files is updated
#   join    Join (Like a database join) sorted files (pre sort with -k 1b,1)
#   ncdu    Disk usage navigator
#   paste   Horizontally join files
#   pee     Pipe input to multiple commands (a la tee) (in Ubuntu package 'moreutils')
#   pv      Quantify amount of data going through a pipe ("pipe viewer") (in Ubuntu package 'pv')
#   ts      Prepend a timestamp before each line of stdin (in Ubuntu package 'moreutils')
#   vipe    Run a text-editor to tweak data as it's passed through a pipe (in Ubuntu package 'moreutils')
#
# awk, find, grep, sed, seq, sort, tr, uniq, wc, xargs
#
# (oh-my-)zsh tricks to remember to use:
#   g...  (git ...)
#   la... (ls -lAh)
#   ll... (ls -lh)

# # Append something like this to the prompt: $'\u00A0'
